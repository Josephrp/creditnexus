{
  "name": "client",
  "private": true,
  "version": "0.0.0",
  "main": "electron/main.mjs",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:electron": "wait-on http://127.0.0.1:5000 && electron .",
    "dev:desktop": "concurrently -k \"npm run dev\" \"npm run dev:electron\"",
    "typecheck": "tsc -b",
    "build": "vite build",
    "build:desktop": "npm run build && electron-builder --config electron-builder.yml",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "@finos/fdc3": "^2.2.0",
    "@heroicons/react": "^2.1.0",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-progress": "^1.1.8",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.556.0",
    "moment": "^2.30.1",
    "react": "19.1.0",
    "react-big-calendar": "^1.19.4",
    "react-dom": "19.1.0",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^6.28.0",
    "tailwind-merge": "^3.4.0",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/leaflet": "^1.9.12",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-big-calendar": "^1.16.3",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "electron": "^39.2.7",
    "electron-builder": "^26.4.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "eslint-plugin-security": "^3.0.1",
    "globals": "^16.5.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.1.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "^7.2.4",
    "wait-on": "^9.0.3"
  },
  "overrides": {
    "react-leaflet": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@react-leaflet/core": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}